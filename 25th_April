  import java.util.Scanner;

public class switch_demo {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		int ch;
		Scanner sc=new Scanner(System.in);
		System.out.println("Ente your choice \n 1:Withdraw \t 2:Pin change \n 3:Deposit \t 4:Balance");
		ch=sc.nextInt();
		switch(ch)
		{
		case 1:
			System.out.println("Perofrm the withdraw opration");
			break;
		case 2:
			System.out.println("Perofrm the pin change opration");
			break;
		case 3:
			System.out.println("Perofrm the credi  opration");
			break;
		case 4:
			System.out.println("Perofrm the balance opration");
			break;
		default:
			System.out.println("Invalid choice");
			break;
		}
		
		

	}

}

